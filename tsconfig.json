{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "strict": true,
        "suppressImplicitAnyIndexErrors": true,
        "noUnusedLocals": true,
        "noImplicitThis": false,
        "forceConsistentCasingInFileNames": true,
        "removeComments": true,
        "allowUnreachableCode": true,
        "allowJs": true,
        "lib": [
            "es2016",
            "dom"
        ],
        "sourceMap": true,
        "rootDir": "."
    },
    "exclude": [
        ".vscode-test",
        "node_modules",
        "ext",
        "test",
        "out"
    ]
}